{
	"author": "Kevin Peckham",
	"name": "lj-sveltekit-ts-tailwind-pug-starter",
	"version": "1.0.4",
	"repository": {
		"type": "git",
		"url": "https://github.com/lightning-jar/svelte-typescript-tailwind-pug-starter"
	},
	"devDependencies": {
		"@playwright/test": "^1.33.0",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/kit": "^1.16.3",
		"@typescript-eslint/eslint-plugin": "^5.59.5",
		"@typescript-eslint/parser": "^5.59.5",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.28.0",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"@prettier/plugin-pug": "^2.5.0",
		"pug": "^3.0.2",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.5",
		"vitest": "^0.31.0"
	},
	"description": "A Svelte, Typescript, Tailwindcss, & Pug starter template",
	"homepage": "https://skinnypug.dev",
	"keywords": [
		"Svelte",
		"Typescript",
		"TailwindCSS",
		"Pug",
		"Starter"
	],
	"license": "MIT",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"test:unit": "vitest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"engines": {
		"node": "18.x",
		"pnpm": "8.x"
	},
	"type": "module"
}
